@model IEnumerable<OSLibrary.Models.Category>
@{

    Layout = null;
}

<div class="col-2 side">
    <ul  style="padding-left:0;">
        @foreach (var item in Model)
        {
            <li>
                <a href="@Url.Action("Index","Products",new { @item.CategoryName , ViewBag.Gender})">@item.CategoryName</a>
            </li>
        }
    </ul>
</div>